<table id="usertable" class="table table-striped" style="width:100%">
  <thead>
    <tr>
      {% for column in header %}
        <th>{{ column }}</th>
      {% endfor %}
    </tr>
  </thead>
  <tbody>
    {% for row in rows %}
      <tr>
        {% for column in row %}
          <td>{{ column }}</td>
        {% endfor %}
      </tr>
    {% endfor %}
  </tbody>
  <tfoot>
    <tr>
      {% for column in header %}
        <th>{{ column }}</th>
      {% endfor %}
    </tr>
  </tfoot>
</table>
